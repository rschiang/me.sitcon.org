<div class="staffs">
  <div class="container">
    <h2>SITCON Staffs</h2>
  </div>
  <aside class="staff" ng-repeat="(id, staff) in staffList">
    <div class="avatar">
      <a ng-href="/#/{{id}}">
        <img src="{{staff.avatar}}&s=32" class="light ellipse" alt="{{staff.name}}" />
      </a>
    </div>
    <h4>
      <a ng-href="/#/{{id}}">
        {{staff.name}}
        <span class="sub-header">{{staff.title}}</span>
      </a>
    </h4>
  </aside>
</div>
<div class="background"></div>
